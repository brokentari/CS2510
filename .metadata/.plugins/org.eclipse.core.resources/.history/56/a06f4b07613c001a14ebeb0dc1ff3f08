interface GamePiece {
  int getValue();
}

class BaseTile implements GamePiece {
  int value;
  
  BaseTile(int value) {
    this.value =  value;
  }

  @Override
  public int getValue() {
    // TODO Auto-generated method stub
    return this.value;
  }
  
}

class MergedTile implements GamePiece {
  GamePiece piece1;
  GamePiece piece2;
  
  MergedTitle implements GamePiece {
    this.piece1 = piece1;
    this.piece2 = piece2;
  }
  @Override
  public int getValue() {
    // TODO Auto-generated method stub
    return 0;
  }
}